	year = {2017},
	note = {NTRS Author Affiliations: NASA Langley Research Center
NTRS Report/Patent Number: NF1676L-25486
NTRS Document ID: 20170001569
NTRS Research Center: Langley Research Center (LaRC)},
	keywords = {Structural Mechanics},
