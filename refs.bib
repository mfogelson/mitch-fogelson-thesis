
@article{nagaraj_kinematics_2009,
	title = {Kinematics of pantograph masts},
	volume = {44},
	issn = {0094-114X},
	url = {https://www.sciencedirect.com/science/article/pii/S0094114X08000876},
	doi = {10.1016/j.mechmachtheory.2008.04.004},
	abstract = {This paper deals with the kinematics of pantograph masts. Pantograph masts have widespread use in space application as deployable structures. They are over constrained mechanisms with degree-of-freedom, evaluated by the Grübler–Kutzback formula, as less than one. In this paper, a numerical algorithm is used to evaluate the degree-of-freedom of pantograph masts by obtaining the null space of a constraint Jacobian matrix. In the process redundant joints in the masts are obtained. A method based on symbolic computation, to obtain the closed-form kinematics equations of triangular and box shaped pantograph masts, is presented. In the process, the various configurations such masts can attain during deployment, are obtained. The closed-form solution also helps in identifying the redundant joints in the masts. The symbolic computations involving the Jacobian matrix also leads to a method to evaluate the global degree-of-freedom for these masts.},
	language = {en},
	number = {4},
	urldate = {2022-11-17},
	journal = {Mechanism and Machine Theory},
	author = {Nagaraj, B. P. and Pandiyan, R. and Ghosal, Ashitava},
	month = apr,
	year = {2009},
	keywords = {Closed-form solutions, Degree-of-freedom, Deployable pantograph masts, Jacobian matrix},
	pages = {822--834},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\sawyer\\Zotero\\storage\\YE5EDMKH\\Nagaraj et al. - 2009 - Kinematics of pantograph masts.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\sawyer\\Zotero\\storage\\XCIF7B5U\\S0094114X08000876.html:text/html},
}

@article{kim_deployable_2021,
	title = {Deployable truss structure with flat-form storability using scissor-like elements},
	volume = {159},
	issn = {0094-114X},
	url = {https://www.sciencedirect.com/science/article/pii/S0094114X21000100},
	doi = {10.1016/j.mechmachtheory.2021.104252},
	abstract = {Deployable structures have received much attention in the building of large structural systems in space due to the limitations in the packaging capabilities of satellites and launch vehicles. Among various categories of deployable structures, deployable truss structures are most commonly used because of their high stiffness and strength. However, deployable truss structures typically have low packaging efficiency. To improve the packaging efficiency of deployable truss structures, this paper proposes a deployable truss structure that can be stored in a flat form. The proposed structure has a high packaging efficiency while preserving the major advantage of conventional deployable truss structures. The deployment motion is analyzed through a kinematic model, and the packaging efficiency is calculated and compared to a conventional deployable truss structure. The dynamic model of the proposed structure is derived and the deployment characteristics are investigated considering the driving force for deployment and gravity. Finally, the feasibility of the proposed structure is demonstrated with a deployment test of the prototype.},
	language = {en},
	urldate = {2022-11-17},
	journal = {Mechanism and Machine Theory},
	author = {Kim, Tae-Hyun and Suh, Jong-Eun and Han, Jae-Hung},
	month = {5},
	year = {2021},
	keywords = {Deployable truss structure, Dynamic model, Flat-form storability, Kinematic analysis, Packaging efficiency, Scissor-like element (SLE)},
	pages = {104252},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\sawyer\\Zotero\\storage\\UEYIM5GG\\Kim et al. - 2021 - Deployable truss structure with flat-form storabil.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\sawyer\\Zotero\\storage\\ZL3VIS29\\S0094114X21000100.html:text/html},
}

